{
  "name": "web-desktop",
  "version": "0.7.56",
  "scripts": {
    "cleanold": "rm -rf .next out",
    "clean": "rimraf -rf .next out",
    "env:dev": "cp ./environments/.env.dev ./.env",
    "env:alpha": "cp ./environments/.env.alpha ./.env",
    "env:beta": "cp ./environments/.env.beta ./.env",
    "env:prod": "cp ./environments/.env.prod ./.env",
    "dev": "yarn clean && yarn env:dev && next",
    "devwin": "yarn clean && next",
    "build:shortcut": "yarn clean && next build && next export",
    "build:dev": "yarn env:dev && yarn build:shortcut",
    "build:alpha": "yarn env:alpha && yarn build:shortcut",
    "build:beta": "yarn env:beta && yarn build:shortcut",
    "build:prod": "yarn env:prod && yarn build:shortcut",
    "start:dev": "NODE_ENV=production next start -p 8000",
    "start:prod": "NODE_ENV=production next start -p 80",
    "type-check": "tsc",
    "lint-check": "tsc --noEmit && eslint --ignore-path ./.eslintignore --fix",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./public",
    "storybook:build": "build-storybook -c .storybook -o .storybook/dist -s ./public",
    "package:install": "yarn && git submodule foreach yarn",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "checkout": "scripts/checkout.sh",
    "push": "scripts/push.sh",
    "branch": "scripts/branch.sh",
    "branch:D": "scripts/delete.sh"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.25",
    "@material-ui/lab": "^5.0.0-alpha.25",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-jest": "^26.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-app": "^9.1.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "cpx": "^1.5.0",
    "dayjs": "^1.9.6",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "isomorphic-unfetch": "3.0.0",
    "jest": "^26.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "next": "^9.5.5",
    "next-images": "^1.3.1",
    "next-redux-wrapper": "^5.0.0",
    "polished": "^3.6.5",
    "qrcode.react": "^1.0.0",
    "query-string": "^6.12.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-download-link": "^2.3.0",
    "react-lottie": "^1.2.3",
    "react-redux": "7.2.0",
    "react-to-print": "^2.9.0",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@material-ui/core": "^5.0.0-alpha.25",
    "@material-ui/lab": "^5.0.0-alpha.25",
    "@storybook/addon-actions": "^6.0.17",
    "@storybook/addon-docs": "^6.0.17",
    "@storybook/addon-knobs": "^6.0.17",
    "@storybook/addon-viewport": "^6.0.17",
    "@storybook/react": "^6.0.17",
    "@types/classnames": "^2.2.10",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.12.21",
    "@types/qrcode.react": "^1.0.1",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-lottie": "^1.2.3",
    "@types/react-redux": "^7.1.7",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.6",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.2",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-router-dom": "^5.1.2",
    "redux-devtools-extension": "^2.13.8",
    "styled-components": "^5.0.1",
    "styled-tools": "^1.7.1",
    "typescript": "^3.9.7"
  },
  "license": "ISC"
}
